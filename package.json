{
  "name": "dsc-spectrum",
  "version": "0.4.1",
  "description": "Deal with DSC (Differential Scanning Calorimetry) data",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build  --entry src/index.js --root DSCSpectrum",
    "build-doc": "cheminfo doc",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run test-import",
    "test-only": "vitest run --coverage",
    "test-import": "node -e \"import('file://' + process.cwd() + '/src/index.js').then(console.log)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/dsc-spectrum.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/dsc-spectrum/issues"
  },
  "homepage": "https://github.com/cheminfo/dsc-spectrum#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "cheminfo-build": "^1.3.2",
    "eslint": "^9.39.2",
    "eslint-config-cheminfo": "^17.1.0",
    "prettier": "^3.8.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "common-spectrum": "3.1.0",
    "physical-chemistry-parser": "^3.4.2"
  }
}
